Using namespace std;
Int main()
{
Float n, p;
Int i;
Cout << ('n=') ;
Cin >> n;
If (n mod 2=1);
I=1;
Else
I=2;
While (i<=n) {
P*=i;
I+=2} 
Cout << (n, '!! =' P) ;
} 
6
#include <iostream>
using namespace std;
int main()
{
 unsigned long long k = 1; // k = зерна вначале 
 unsigned long long s = 0; // s = всего зёрен
// i = клетка 
 
 for ( int i=1; i<=64; i++){
 s += k;
 cout << i << "; k=" << k << "; s=" << s << endl;
 k = k * 2;
 }
}
