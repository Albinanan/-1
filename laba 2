#include <iostream>
#include <cmath>
using namespace std;
int main()
{
float L, i, B, F, sin;
cin >> L >> i >> B;
F = L*i*B*sin;
cout << F;
}
2
#include <iostream>
#include <cmath>
using namespace std;
int main()
{
int r=6350, h, S;
cin >> h;
S=sqrt(h*h)+sqrt(r*r);
cout << S;
}
3
#include<stdio.h>
unsigned int doublefactorial(unsigned int n)
{
 if (n == 0 || n==1)
 return 1;
 return n*doublefactorial(n-2);
}
int main()
{
 printf("Double factorial is %d", doublefactorial(7));
 return 0;
}
